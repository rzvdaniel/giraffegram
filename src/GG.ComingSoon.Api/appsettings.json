  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppConfig": {
    "DatabaseType": "MySql"
  },
  "AllowedHosts": "*"
}
